<section id="map" class="wow fadeInUp" data-wow-delay="0.3s">
  <div class="container">
    {{content.field_map.0}}
  </div>
</section>
<!--  End map section -->
<!-- Contact form and address  -->
<section id="contact">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <div class="contact-info wow fadeInUp" data-wow-delay="0.3s">
          <h3>{{content.field_address_header.0}}</h3>
          <p>{{content.field_address.0}}</p>
        </div>  
        <div class="contact-info wow fadeInUp" data-wow-delay="0.6s">
          <h3>{{content.field_reach_us.0}}</h3>
          <p><a href="tel:{{content.field_phone.0}}">{{content.field_phone.0}}</a></p>
        </div> 
        <div class="contact-info wow fadeInUp" data-wow-delay="0.9s">
          <h3>{{content.field_email_header.0}}</h3>
          <p><a href="mailto:{{content.field_mail.0}}">{{content.field_mail.0}}</a></p>
        </div> 
      </div>
      <div class="col-md-6 wow fadeInUp" data-wow-delay="0.5s">
        <h2>{{content.field_form_title.0}}</h2>
          {{ content.field_contact_form.0 }}
      </div>
    </div>
  </div>  
</section>